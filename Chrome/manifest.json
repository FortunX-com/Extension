{
	"manifest_version": 3,
	"name": "Thanx Finance",
	"version": "1.0.1",
	"key": "",
	"oauth2": {
		"client_id": "39029048073-7pq26147ts917og78qa9up0tfilrubdf.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/contacts.readonly",
			"https://www.googleapis.com/auth/userinfo.email",
			"https://www.googleapis.com/auth/userinfo.profile"
		]
	},
	"icons": {
		"16": "assets/icons/icon16.png",
		"48": "assets/icons/icon48.png",
		"128": "assets/icons/icon128.png"
	},
	"permissions": [
		"tabs", "identity"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"libs/jquery-3.6.0.min.js",
				"libs/jsrsasign-all-min.js",
				"utils/utility.js",
				"utils/http.js",
				"content_extension.js",
				"extension_bot.js"
			],
			"css": [
				"style.css",
				"modal/modal/modal.css",
				"modal/gmail/gmail.css",
				"modal/chrome-update/chrome-update.css"
			],
			"run_at": "document_end",
			"all_frames": true
		}
	],
	"action": {
		"default_icon": "assets/icons/icon16.png",
		"default_title": "Thanx Finance"
	},
	"background": {
		"service_worker": "background.js"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"app/index.html",
				"libs/jquery-3.6.0.min.js",
				"libs/jsrsasign-all-min.js",
				"utils/json_csv.js",
				"style.css",
				"utils/utility.js",
				"utils/http.js",
				"popup/popup.js",
				"extension_bot.js",
				"modal/modal/modal.html",
				"modal/chrome-update/chrome-update.html"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}



